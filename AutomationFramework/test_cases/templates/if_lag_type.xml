  <edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
      <interfaces xmlns="http://openconfig.net/yang/interfaces">
        <interface>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
            <type>{{type}}</type>
            <enabled>{{enabled}}</enabled>
          </config>
          <aggregation xmlns="http://openconfig.net/yang/interfaces/aggregate">
            <config>
              <lag-type>{{lag_type}}</lag-type>
            </config>
          </aggregation>
        </interface>
      </interfaces>
    </config>
  </edit-config>
